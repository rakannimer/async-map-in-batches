{
  "name": "async-map-in-batches",
  "private": false,
  "version": "0.0.0-development",
  "main": "dist/index.js",
  "module": "dist/index.m.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "npm run test:i",
    "build": "rm -rf ./dist/ && rollup -c",
    "test:i": "jest --watch --coverage",
    "test": "jest --coverage",
    "prepublish": "npm run test && npm run build",
    "commit": "git-cz",
    "setup:semantic-release": "npx semantic-release-cli setup",
    "semantic-release": "semantic-release"
  },
  "husky": {
    "pre-commit": "prettier src/* --write",
    "pre-push": "npm run test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rakannimer/async-map-in-batches"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^23.3.3",
    "comment-json": "^1.1.3",
    "git-cz": "^1.7.1",
    "husky": "^1.1.1",
    "jest": "^23.6.0",
    "parcel-bundler": "^1.10.1",
    "prettier": "^1.14.3",
    "rollup": "^0.66.4",
    "rollup-plugin-typescript2": "^0.17.1",
    "semantic-release": "^15.9.17",
    "ts-jest": "^23.10.3",
    "typescript": "^3.1.1"
  },
  "jest": {
    "preset": "ts-jest",
    "collectCoverageFrom": [
      "src/*.{ts,tsx}"
    ]
  },
  "description": "Map an array to another with batched async operations."
}
