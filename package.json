{
  "name": "async-map-in-batches",
  "private": false,
  "version": "0.0.0-development",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "npm run test:i",
    "build": "rm -rf ./dist/ && rollup -c",
    "test:i": "jest --watch --coverage",
    "test": "jest --coverage",
    "prepublish": "npm run test && npm run build",
    "commit": "git-cz",
    "setup:semantic-release": "npx semantic-release-cli setup",
    "semantic-release": "semantic-release"
  },
  "husky": {
    "pre-commit": "prettier src/* --write",
    "pre-push": "npm run test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rakannimer/async-map-in-batches"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^24.0.15",
    "comment-json": "^2.1.0",
    "git-cz": "^3.2.1",
    "husky": "^3.0.0",
    "jest": "^24.8.0",
    "parcel-bundler": "^1.12.3",
    "prettier": "^1.18.2",
    "rollup": "^1.16.7",
    "rollup-plugin-typescript2": "^0.22.0",
    "semantic-release": "^17.2.3",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.3"
  },
  "jest": {
    "preset": "ts-jest",
    "collectCoverageFrom": [
      "src/*.{ts,tsx}"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/build/",
      "/dist/"
    ]
  },
  "description": "Map an array to another with batched async operations."
}
